{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "transform-react-pug",
        "@babel/transform-react-jsx",
        ["@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
          ]
    ]
}